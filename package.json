{
	"name": "tabbie",
	"description": "The missing tab manager for Chrome",
	"private": true,
	"main": "index.js",
	"dependencies": {
		"@bufferapp/redux-hover": "^0.0.6",
		"babel-preset-env": "^1.5.2",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "6.24.1",
		"babelify": "^7.3.0",
		"browserify": "^14.4.0",
		"cross-env": "^5.0.1",
		"glob": "^7.1.2",
		"loose-envify": "^1.3.1",
		"node-zip": "^1.1.1",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-redux": "^5.0.5",
		"react-router": "^3.0.2",
		"redux": "^3.7.1",
		"redux-thunk": "^2.2.0",
		"uglify-es": "^3.0.23",
		"url-parse": "^1.1.9",
		"watchify": "^3.9.0"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production browserify ./src/index.jsx --extension=jsx -t babelify | uglifyjs -c > dist/bundle.js",
		"start": "watchify ./src/index.jsx --extension=jsx -v -t babelify -o dist/bundle.js",
		"zip": "npm run build && node zip"
	}
}